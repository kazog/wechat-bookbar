        "pages/sort/list/list",
        "pages/my/about/about",
        "pages/my/setting/setting"
    ],
    "subpackages": [
        {
            "root": "supack",
            "pages": [
                "pages/index/index",
                "pages/book/book",
                "pages/order/order",
                "pages/prize/prize",
                "pages/author/author"
            ]
        }
    ],
    "window": {
        "backgroundColor": "#F6F6F6",
        "backgroundTextStyle": "light",
        "navigationBarBackgroundColor": "#F6F6F6",
        "navigationBarTitleText": "知之书吧",
        "navigationBarTextStyle": "black",
        "navigationStyle": "custom"
    },
    "tabBar": {
        "color": "#909090",
        "selectedColor": "#121212",
        "list": [
            {
                "selectedIconPath": "images/icon/home_on.png",
                "iconPath": "images/icon/home_off.png",
                "pagePath": "pages/home/home",
                "text": "首页"
            },
            {
                "selectedIconPath": "images/icon/hot_on.png",
                "iconPath": "images/icon/hot_off.png",
                "pagePath": "pages/sort/sort",
                "text": "分类"
            },
            {
                "selectedIconPath": "images/icon/me_on.png",
                "iconPath": "images/icon/me_off.png",
                "pagePath": "pages/my/my",
                "text": "我的"
            }
        ]
    },
    "preloadRule": {
        "pages/home/home": {
            "network": "wifi",
            "packages": [
                "supack"
            ]
        }
    },
    "usingComponents": {
        "empty": "/components/empty/empty",
        "header": "/components/header/header",
        "toptip": "/components/toptips/toptips"
    },
    "requiredBackgroundModes": [
        "audio"
    ],
    "permission": {
        "scope.userLocation": {
            "desc": "您的位置信息将用于小程序推荐附加使用"
        }
    },
    "networkTimeout": {
        "request": 20000,
        "downloadFile": 30000
    },
    "sitemapLocation": "sitemap.json",
    "style": "v2"
